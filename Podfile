pod 'Alamofire'
pod 'Firebase'
pod 'Firebase/Auth'
pod 'Firebase/Database'
pod 'Firebase/Storage'
pod 'SwiftyPickerPopover'
pod 'SwiftyJSON'
pod 'GoogleMaps'
pod 'Material'
pod 'RandomColorSwift'
pod 'SCLAlertView'
pod 'Toucan'
end
